  "TabsDirection": {
    "type": "Radio",
    "value": "horizontal",
    "rules": {
      "horizontal": {
        ".tab-container":"display: block; --full-bg-display: var(--full-bg-display-x);",
        "&.tab-container":"display: block; --full-bg-display: var(--full-bg-display-x)",
        ".nav-tabs": "flex-direction:row; align-items:center; min-width:auto; padding-bottom: var(--pb); overflow-x: var(--overflow-scroll); max-height: none;",
        ".tab-content": "width: 100%; max-height: none; overflow-y: initial; overflow-x: initial; --full-bg-display: block",
        ".nav-tabs + .tab-content": "margin-top: var(--spacing, 0); margin-left:0;"
      },
      "vertical": {
        ".tab-container":"display: flex; --full-bg-display: none",
        "&.tab-container":"display: flex; --full-bg-display: none",
        ".nav-tabs":"flex-direction:column; align-items:initial; min-width:var(--tabs-v-width, 175px); max-height: var(--tabs-v-height, 100%); padding-bottom:0; overflow-x: hidden; overflow-y: auto;",
