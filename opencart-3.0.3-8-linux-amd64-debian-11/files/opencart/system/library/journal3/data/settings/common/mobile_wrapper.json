{
  "MobileWrapperTitleText": {
    "type": "Input",
    "value": "",
    "selector": "[class*='-menu-container'] .mobile-wrapper-header > span::after",
    "property": "content: '%s'"
  },
  "MobileWrapperTitleTextDesktop": {
    "type": "Input",
    "value": "",
    "selector": ".desktop-mobile-main-menu-container .mobile-wrapper-header > span::after",
    "property": "content: '%s'"
  },
  "CartWrapperTitleText": {
    "type": "Input",
    "value": "",
    "selector": ".mobile-cart-content-container .mobile-wrapper-header > span::after",
    "property": "content: '%s'"
  },

  "FilterWrapperTitleText-hold": {
    "type": "Input",
    "value": "",
    "selector": ".mobile-filter-container .mobile-wrapper-header > span::after",
    "property": "content: '%s'"
  },

  "MobileWrapperTitleFont": {
    "type": "Font",
    "selector": ".mobile-wrapper-header > span"
  },

  "MobileWrapperTitleBackground": {
    "type": "Background",
    "selector": ".mobile-wrapper-header"
  },

  "MobileWrapperTitleShadow": {
    "type": "Shadow",
    "selector": ".mobile-wrapper-header"
  },

  "MobileWrapperTitleBorder": {
    "type": "Border",
    "selector": ".mobile-wrapper-header"
  },

  "MobileWrapperCloseIcon": {
    "type": "Icon",
    "selector": ".mobile-wrapper-header > a::before"
  },

  "MobileWrapperCloseIconHover": {
    "type": "Color",
    "selector": ".mobile-wrapper-header > a:hover::before",
    "property": "color"
  },

  "MobileWrapperCloseIconBackground": {
    "type": "Background",
    "selector": ".mobile-wrapper-header > a"
  },

  "MobileWrapperCloseIconBackgroundHover": {
    "type": "Background",
    "selector": ".mobile-wrapper-header > a:hover"
  },

  "MobileWrapperCloseIconBorder": {
    "type": "Border",
    "selector": ".mobile-wrapper-header > a"
  },

  "MobileWrapperCloseIconBorderRadius": {
    "type": "BorderRadius",
    "selector": ".mobile-wrapper-header > a"
  },

  "MobileWrapperTitleHeight": {
    "type": "InputNumber",
    "value": "45",
    "property": "--mobile-wrapper-header: %spx",
    "selector": ".mobile-container"
  },

  "MobileWrapperTitleCloseWidth": {
    "type": "InputNumber",
    "value": "",
    "property": "--mobile-wrapper-header-close: %spx",
    "selector": ".mobile-container"
  },

  "MobileWrapperContentWidthMobileNew": {
    "type": "InputNumber",
    "selector": ".mobile-container",
    "value": "85",
    "property": "max-width:%s%"
  },

  "MobileWrapperContentWidthDesktop": {
    "type": "InputNumber",
    "selector": ".mobile-container",
    "value": "400",
    "property": "width:%spx"
  },

  "MobileWrapperMenuContentBackground": {
    "type": "Background",
    "selector": ".mobile-main-menu-container.mobile-container, .desktop-mobile-main-menu-container.mobile-container"
  },

  "MobileWrapperFilterContentBackground": {
    "type": "Background",
    "selector": ".mobile-filter-container.mobile-container"
  },

  "MobileWrapperFilterPadding": {
    "type": "Padding",
    "selector": ".mobile-filter-wrapper .module-filter"
  },

  "MobileWrapperMenuPadding": {
    "type": "Padding",
    "selector": ".mobile-main-menu-wrapper .main-menu, .desktop-mobile-main-menu-wrapper .main-menu"
  },

  "MobileWrapperCartPadding": {
    "type": "Padding",
    "selector": ".mobile-cart-content-wrapper .cart-products table"
  },
  "MobileWrapperContentBackground": {
    "type": "Background",
    "selector": ".mobile-container"
  },

  "MobileWrapperCartContentBackground": {
    "type": "Background",
    "selector": ".mobile-cart-content-container.mobile-container"
  },

  "MobileWrapperCartIcon": {
    "type": "Icon",
    "selector": ".mobile-cart-content-container .mobile-wrapper-header > span::before"
  },

  "MobileWrapperMenuIcon": {
    "type": "Icon",
    "selector": ".mobile-main-menu-container .mobile-wrapper-header > span::before, .desktop-mobile-main-menu-container .mobile-wrapper-header > span::before"
  },
  "MobileWrapperHeaderTitleBackground": {
    "type": "Background",
    "selector": ".mobile-wrapper-header > span"
  },
  "MobileMenuLabelOffset": {
    "type": "InputPair",
    "selector": ".mobile-wrapper-header > span",
    "properties": {
      "first": "--offset-left:%spx",
      "second": "--offset-right:%spx"
    }
  },

  "MobileWrapperFilterIcon": {
    "type": "Icon",
    "selector": ".mobile-filter-container .mobile-wrapper-header > span::before"
  },

  "MobileWrapperMenuShadow": {
    "type": "Shadow",
    "selector": "html[class*='-container-open'] .mobile-main-menu-container, html[class*='-container-open'] .desktop-mobile-main-menu-container"
  },

  "MobileWrapperCartShadow": {
    "type": "Shadow",
    "selector": "html[class*='-container-open'] .mobile-cart-content-container"
  },

  "MobileWrapperFilterShadow": {
    "type": "Shadow",
    "selector": "html[class*='-container-open'] .mobile-filter-container"
  },

  "MobileWrapperBackdrop": {
    "type": "Background",
    "selector": "&.mobile-overlay .site-wrapper::before",
    "important": "true"
  },
  "TabsDirection": {
    "type": "Toggle",
    "value": "",
    "rules": {
      "true": {
        ".mobile-container .tab-container":"display: block !important;",
        ".mobile-container ul.nav-tabs": "flex-direction:row !important; min-width:auto !important; overflow-x: var(--overflow) !important; padding-bottom: var(--pb) !important;",
        ".mobile-container .tab-content": "width: 100% !important;",
        ".mobile-container .nav-tabs + .tab-content": "margin-top: var(--spacing, 0) !important; margin-left:0 !important;"
      }
    }
  },
  "OffCanvasEmptyIcon-hold": {
    "type": "Icon",
    "selector": ".mobile-main-menu-wrapper:empty::before, .desktop-mobile-main-menu-wrapper:empty::before"
  },
  "OffCanvasEmptyText-hold": {
    "type": "InputLang",
    "value": "",
    "selector": ".mobile-main-menu-wrapper:empty::after, .desktop-mobile-main-menu-wrapper:empty::after",
    "property": "content: '%s'"
  }
}
