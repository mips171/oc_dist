    "type": "InputPair",
    "selector": ".lg-outer .lg-toggle-thumb",
    "properties": {
      "first": "width: %spx; height: %spx",
      "second": "height: %spx"
    }
  },
  "ArrowsIconSize": {
    "type": "InputPair",
    "selector": ".fancybox__nav .carousel__button",
    "properties": {
      "first": "width: %spx; height: %spx",
      "second": "height: %spx"
    }
  },
  "ThumbToggleStatus": {
    "type": "Toggle",
    "value": "false",
    "js": true,
    "php": true
  },
  "ThumbIconPositionNew": {
    "type": "Radio",
    "value": "right",
    "rules": {
      "left": {
        ".lg-outer .lg-toggle-thumb": "left:20px; right:auto; transform: translate3d(0,-100%,0);"
      },
      "center": {
        ".lg-outer .lg-toggle-thumb": "left:50%; right:auto; transform: translate3d(-50%,-100%,0);"
      },
      "right": {
        ".lg-outer .lg-toggle-thumb": "left:auto; right:20px; transform: translate3d(0,-100%,0);"
      }
    },
    "rtlRules": {
      "left": {
        ".lg-outer .lg-toggle-thumb": "right:20px; left:auto; transform: translate3d(0,-100%,0);"
      },
      "center": {
        ".lg-outer .lg-toggle-thumb": "right:50%; left:auto; transform: translate3d(-50%,-100%,0);"
      },
      "right": {
        ".lg-outer .lg-toggle-thumb": "right:auto; left:20px; transform: translate3d(0,-100%,0);"
      }
    }
  },
