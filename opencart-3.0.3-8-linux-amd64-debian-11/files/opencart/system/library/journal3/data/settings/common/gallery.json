  "AutoPlayVisibility": {
    "type": "Toggle",
    "value": "false",
    "rules": {
      "true": {
        ".fancybox__button--slideshow": "display: flex"
      },
      "false": {
        ".fancybox__button--slideshow": "display: none"
      }
    }
