  },

  "CaptionPosition": {
    "type": "Radio",
    "value": "bottom",
    "rules": {
      "top": {
        ".fancybox__slide": "flex-direction: column-reverse",
        ".fancybox__caption": "padding: 0 0 var(--distance, 1rem) 0"
      },
      "bottom": {
        ".fancybox__slide": "flex-direction: column",
        ".fancybox__caption": "padding: var(--distance, 1rem) 0 0 0"
      }
    }
  },
  "CaptionBottom-Legacy": {
    "type": "InputNumber",
    "value": "90",
